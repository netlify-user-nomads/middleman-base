<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
	<meta name="keywords" content="Enterprise Software Management, Oracle, SAP &amp; Enterprise Migrations">
    <link rel="canonical" href="./Enterprise Software Management and Consolidation _ smartShift_files/Enterprise Software Management and Consolidation _ smartShift.html">
	<link rel="shortlink" href="./Enterprise Software Management and Consolidation _ smartShift_files/Enterprise Software Management and Consolidation _ smartShift.html">
	<meta property="og:image" content="http://www.smartshifttech.com/sites/default/files/logo-whitebg.jpg">
	<meta property="og:image:type" content="image/jpeg">
	<meta property="og:image:width" content="276">
	<meta property="og:image:height" content="120">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Enterprise Software Management and Consolidation | smartShift" %></title>
    
    <%= stylesheet_link_tag "css1", "css2", "css3", "css4", "styles.css" %>
    <%= javascript_include_tag  "jquery-1.10.2.min", "js1", "js2", "js3", "js4", "js5", "js6" %>
  </head>
  
  <body class="<%= page_classes %>">
	<div id="slide-nav" class="navbar navbar-default navbar-fixed-top" role="navigation">
		<div class="container-fluid">
			<div class="navbar-header">
				<a class="navbar-toggle">
					<span class="sr-only">Toggle navigation</span>
					<span class="icon-bar"></span><span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</a>
				<% link_to '/' ,  :class => 'navbar-brand' do %>
				<%= image_tag 'logo.png' %>
				<% end %>

			</div>
			<%= partial "/layouts/partials/head" %>
		</div>
	</div>
    <%= yield %>
    <%= javascript_include_tag  "js7" %>
  </body>
</html>